<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <data>

    <!-- Picking View -->
    <record id="view_erefguide_form" model="ir.ui.view">
        <field name="name">erefguide.form</field>
        <field name="model">stock.picking</field>
        <field name="inherit_id" ref="stock.view_picking_form"/>
        <field name="arch" type="xml">
            <xpath expr="//page[@name='extra']/group" position="inside">
              <field name="auth_id"/>
              <field name="picking_number"/>
              <field name="carrier_id"/>
              <group colspan="4" col="2">
                <field name="clave_acceso"/>
                <field name="numero_autorizacion"/>
                <field name="fecha_autorizacion"/>
                <field name="estado_autorizacion"/>
                <field name="ambiente"/>
                <field name="autorizado_sri"/>
              </group>
            </xpath>
        </field>
    </record>

  </data>
</odoo>